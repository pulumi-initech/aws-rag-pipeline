    "type": "module",
    "scripts": {
        "build": "npx tsc --noEmit",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "lint:check": "eslint . --ext .ts --max-warnings 0",
        "test": "mocha test/unit/*.test.ts",
